{
    "name": "cb/qr-code",
    "description": "WordPress QR Code Plugin with local generation",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4",
        "endroid/qr-code": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "ChinmoyBiswas\\CBQRCode\\": "includes/"
        },
        "files": [
            "includes/helpers.php"
        ]
    },
    "config": {
        "platform": {
            "php": ">=7.4"
        },
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
